<?xml version="1.0"?>
 <PropertyList>
 <generic>
    <output>
        <line_separator>newline</line_separator>
        <var_separator>tab</var_separator>
 
         <chunk>
           <node>/orientation/heading-deg</node>
           <name>Heading</name>
           <type>float</type>
           <format>%03.3f</format>
         </chunk>
 
         <chunk>
           <node>/orientation/pitch-deg</node>
           <name>Pitch</name>
           <type>float</type>
           <format>%03.3f</format>
         </chunk>
 
         <chunk>
           <node>/orientation/roll-deg</node>
           <name>Roll</name>
           <type>float</type>
           <format>%03.3f</format>
         </chunk>
    </output>
  </generic>
 </PropertyList>